{
  "name": "solidjs-typewriter",
  "description": "SolidJS TypeWriter",
  "author": "OrJDev",
  "license": "MIT",
  "version": "0.0.8",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/index.jsx",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
